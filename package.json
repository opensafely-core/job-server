{
  "name": "job-server",
  "version": "1.0.0",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "serve": "vite preview",
    "lint": "eslint \"./assets/src/**/*.{js,jsx}\"",
    "lint:fix": "eslint \"./assets/src/**/*.{js,jsx}\" --fix"
  },
  "devDependencies": {
    "@vitejs/plugin-legacy": "^1.4.3",
    "autoprefixer": "^10.2.6",
    "cssnano": "^5.0.6",
    "eslint": "^7.30.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "postcss-inline-svg": "^5.0.0",
    "prettier": "2.3.2",
    "prettier-eslint": "^12.0.0",
    "rollup-plugin-copy": "^3.4.0",
    "sass": "^1.35.1",
    "vite": "^2.3.8"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "bootstrap": "^5.0.2",
    "jquery": "^3.5.1",
    "list.js": "^2.3.0",
    "prop-types": "^15.7.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-query": "^3.18.1",
    "select2": "^4.0.13"
  },
  "eslintConfig": {
    "extends": [
      "airbnb",
      "prettier"
    ],
    "plugins": [
      "prettier"
    ],
    "rules": {
      "prettier/prettier": "error",
      "react/jsx-props-no-spreading": "off",
      "react/jsx-sort-props": [
        "error",
        {
          "ignoreCase": true,
          "reservedFirst": true
        }
      ],
      "import/order": [
        "error",
        {
          "groups": [
            [
              "builtin",
              "external",
              "internal"
            ],
            "parent",
            "sibling",
            "index"
          ],
          "newlines-between": "never",
          "alphabetize": {
            "order": "asc",
            "caseInsensitive": true
          }
        }
      ]
    },
    "env": {
      "browser": true
    },
    "parserOptions": {
      "ecmaVersion": 2020
    }
  }
}
