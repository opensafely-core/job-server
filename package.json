{
  "name": "job-server",
  "version": "1.0.0",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "serve": "vite preview",
    "lint": "eslint \"./assets/src/**/*.{js,jsx}\"",
    "lint:fix": "eslint \"./assets/src/**/*.{js,jsx}\" --fix"
  },
  "devDependencies": {
    "@vitejs/plugin-legacy": "^1.5.1",
    "autoprefixer": "^10.2.6",
    "cssnano": "^5.0.7",
    "eslint": "^7.32.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.24.0",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "postcss-url": "^10.1.3",
    "prettier": "2.3.2",
    "prettier-eslint": "^13.0.0",
    "rollup-plugin-copy": "^3.4.0",
    "rollup-plugin-visualizer": "^5.5.2",
    "sass": "^1.37.5",
    "vite": "^2.4.4"
  },
  "dependencies": {
    "@sentry/react": "^6.11.0",
    "bootstrap": "^4.5.2",
    "convert-csv-to-json": "^1.3.1",
    "jquery": "^3.5.1",
    "list.js": "^2.3.0",
    "lodash": "^4.17.21",
    "pretty-bytes": "^5.6.0",
    "prop-types": "^15.7.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-papaparse": "^3.17.1",
    "react-query": "^3.19.4",
    "react-router-dom": "^5.2.0",
    "react-window": "^1.8.6",
    "select2": "^4.0.13",
    "whatwg-fetch": "^3.6.2",
    "zustand": "^3.5.7"
  },
  "eslintConfig": {
    "extends": [
      "airbnb",
      "airbnb/hooks",
      "prettier"
    ],
    "plugins": [
      "prettier"
    ],
    "rules": {
      "prettier/prettier": "error",
      "react/jsx-props-no-spreading": "off",
      "react/jsx-sort-props": [
        "error",
        {
          "ignoreCase": true,
          "reservedFirst": true
        }
      ],
      "import/order": [
        "error",
        {
          "groups": [
            [
              "builtin",
              "external",
              "internal"
            ],
            "parent",
            "sibling",
            "index"
          ],
          "newlines-between": "never",
          "alphabetize": {
            "order": "asc",
            "caseInsensitive": true
          }
        }
      ]
    },
    "env": {
      "browser": true
    },
    "parserOptions": {
      "ecmaVersion": 2020
    }
  }
}
